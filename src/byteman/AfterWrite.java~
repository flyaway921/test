public class AfterWrite{
	public int member;
	public static void main(String[] args){
	 try{
	  Thread.sleep(20000);
	}catch (Exception e){
		e.printStackTrace();
	}
	  System.out.println("before method");
	  new AfterWrite().method();
	  System.out.println("after method");
	}
	
	public void method(){
		final int current;
		System.out.println("this is method");
		//System.out.println("before write: "+iii);
		current=10;
		this.member=20;
		//System.out.println("after write"+iii);
	
	}

}
